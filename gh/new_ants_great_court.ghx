in_valence = 6 # the minimum valence of a point not on the edge of the mesh.
# this value should change for square (4) and triangular (6) meshes

def mesh_neighborhood(m):
    nbrhd = []
    for pi in range(len(m.pts)): # for each point index in our mesh
        nbrhd.append([]) #append an empty list of neighbors to hold neighbor information for this point.
        for face in m.faces: # look at each face in the mesh
            if pi in face : # if we find this point index listed in the face
                for n in range(len(face)):
                    if face[n] != pi and face[n] not in nbrhd[-1]: nbrhd[-1].append(face[n])
        if len(nbrhd[-1]) &lt; in_valence : nbrhd[-1] = False
    return nbrhd

a.put(mesh_neighborhood)</item>
