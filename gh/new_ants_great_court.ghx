                  <items count="7">
                    <item name="CodeInput" type_name="gh_string" type_code="10">from Rhino.Geometry import Point3d

def test(h_val,n_vals):
    try:
        p = h_val.Value
    except:
        p = h_val
        
    n_pt = []
    
    sx = 0.0
    sy = 0.0
    sz = 0.0
    
    n = len(n_vals)
    
    if n &lt; 5:
        return h_val
    
    for i in range(n):
        try:
            n_pt.append(n_vals[i].Value)
        except:
            n_pt.append(n_vals[i])
        sx+=n_pt[i].X
        sy+=n_pt[i].Y
        sz+=n_pt[i].Z
    
    return Point3d(sx/n, sy/n, sz/n)
    
a = test</item>
