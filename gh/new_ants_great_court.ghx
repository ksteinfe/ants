nbrhd = func(msh)# calculate mesh neighborhood

for step in range(steps): # sets the number of smoothing steps
    edge_pt_cnt = 0
    new_msh = Mesh()#create a new mesh
    for pi in range(len(msh.pts)): # for each point index in the old mesh
        if (nbrhd[pi]): # if the point is not an edge condition
             # find the centroid of all its neighboring points and add that to 
             # the new mesh in the indexed position
             neighbors = [msh.pts[n] for n in nbrhd[pi]]
             new_msh.append(Point.centroid(neighbors)) 
        else: 
            # if the point is an edge condition, add it to the new mesh
            new_msh.append(msh.pts[pi])
            edge_pt_cnt += 1
            
    for face in msh.faces: #for each face in the old mesh
        #build a new mesh face with the new point locations
        new_msh.add_face(face[0], face[1], face[2])
    msh = new_msh #overwrite the old mesh 
    
    

