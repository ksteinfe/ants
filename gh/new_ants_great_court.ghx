in_valence = 4 # the minimum valence of a point not on the edge of the mesh.
# this value should change for square (4) and triangular (6) meshes

def mesh_neighborhood_quad(m):
    nbrhd = []
    
    for pi in range(len(m.pts)): # for each point index in our mesh
        nbrhd.append([]) #append an empty list of neighbors to hold neighbor information for this point.
        for face in m.faces: # look at each face in the mesh
            if pi in face : # if we find this point index listed in the face
                # add the two other points found in this face that are adjacent to this face
                index_in_face = face.index(pi)
                nbrhd[-1].append(face[index_in_face-1])
                if index_in_face == 3 : index_in_face = -1
                nbrhd[-1].append(face[index_in_face+1])
        nbrhd[-1] = list(set(nbrhd[-1])) # remove duplicates
        if len(nbrhd[-1]) &lt; in_valence : 
            nbrhd[-1] = False
        
    return nbrhd

