def func(t):
    c = math.cos(t)
    s = math.sin(t)
    alpha = math.atan(rec_h/rec_w)
    pi = math.pi
    pi_over_2 = 0.5*pi
    
    if (t &gt;=0) and (t &lt; alpha):
        l_coord = 0.5*rec_w*s/c
        pt_out = Point(rec_w/2, l_coord)
    elif (t &gt;= alpha) and (t &lt; pi-alpha):
        l_coord = 0.5*rec_h*c/s
        pt_out = Point(l_coord, rec_h/2)
    elif (t &gt;= pi-alpha) and (t &lt; pi + alpha):
        l_coord = -0.5*rec_w*s/c
        pt_out = Point(-rec_w/2, l_coord)
    elif (t &gt;= pi + alpha) and (t &lt; 2*pi - alpha):
        l_coord = -0.5*rec_h*c/s
        pt_out = Point(l_coord, -rec_h/2) 
    elif (t &gt;= 2*pi-alpha) and (t &lt;= 2*pi):
        l_coord = 0.5*rec_w*s/c
        pt_out = Point(rec_w/2, l_coord)
    else:
        print "Out-of-bounds!"
   
    return pt_out
     

